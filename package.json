  "extensionDependencies": [
    "JuanBlanco.solidity",
    "ms-vscode.azure-account"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run compile"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint -c .eslintrc.js --ext .ts src/**/*.ts test/**/*.ts",
      "prettier --write",
      "git add"
    ]
  }
}
